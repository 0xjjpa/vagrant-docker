# -*- mode: ruby -*-
# vi: set ft=ruby :

$name = ENV['name'] + 'Host'

Vagrant.configure(2) do |config|
  config.vm.box = "ailispaw/docker-root"
  config.vm.provision "docker"
  config.vm.network "forwarded_port", guest: 80, host: 8080, auto_correct: true

  config.vm.define $name do |it|
  end
end
